{
    "manifest_version": 2,
    "name": "Url Shortcut",
    "description": "This extension helps you go to the page you want to go without typing out the whole url",
    "version": "1.0",

    "omnibox": { "keyword" : "ii" },
    "icons": {
        "16": "icon.png"
    },
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "JIRA Linker"
    },
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/",
        "storage"
    ]
}
